  "scripts": {
    "vite:build": "vite build src --config ./config/vite/vite.config.js",
    "vite:dev": "vite src --config ./config/vite/vite.config.js",
    "vite:preview": "vite preview dist --config ./config/vite/vite.config.js",
    "lint": "./node_modules/eslint/bin/eslint.js -c ./config/eslint/.eslintrc.js 'src/**/*.{ts,tsx}' --ignore-pattern '*.test.*'",
    "start": "serve -l 3000 -s dist",
    "jest": "jest --config ./config/jest/jest.config.json",
    "jest:watch": "npm run test -- --watch",
    "test:deprecated": "npm run lint && npm run jest",
    "test": "vitest --config ./config/vite/test.config.js",
    "prettier:write": "prettier --write ./src"
  },
