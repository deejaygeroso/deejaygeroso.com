  "scripts": {
    "build": "vite build src --config ./config/vite/vite.config.js",
    "dev": "vite src --config ./config/vite/vite.config.js",
    "preview": "vite preview dist --config ./config/vite/vite.config.js",
    "lint": "./node_modules/eslint/bin/eslint.js -c ./config/eslint/.eslintrc.cjs 'src/**/*.{ts,tsx}' --ignore-pattern '*.test.*'",
    "start": "serve -l 3000 -s dist",
    "test": "vitest --config ./config/vite/test.config.js",
    "test:coverage": "vitest --config ./config/vite/test.config.js --coverage",
    "check": "pnpm lint && pnpm test",
    "prettier:write": "prettier --write ./src"
  },
  "repository": {
    "type": "git",
