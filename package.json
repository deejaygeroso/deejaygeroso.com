    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "jest": "^27.0.6",
    "prettier": "^2.0.5",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.4",
    "vite": "^4.1.1",
