    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "jsdom": "^21.1.0",
    "prettier": "^2.8.4",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4"
  },
  "dependencies": {
    "path": "^0.12.7",
    "react": "^18.2.0",
